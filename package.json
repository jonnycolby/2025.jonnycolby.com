{
    "name": "2025.jonnycolby.com",
    "version": "2025.3.16",
    "private": true,
    "scripts": {
        "dev": "NODE_ENV=development npm run _dev",
        "build": "NODE_ENV=production npm run _build && next build",
        "start": "NODE_ENV=production node -r dotenv/config ./.dist/index.js",
        "lint": "next lint",
        "-": "",
        "_dev": "nodemon --watch ./server --exec npm run _dev:swc",
        "_dev:swc": "npm run _compile && node ./.dist/index.js",
        "_build": "npm run _compile",
        "_compile": "swc ./server --out-dir ./.dist --copy-files --include-dotfiles --strip-leading-paths",
        "--": "",
        "dev:basic": "next dev --turbopack",
        "lint:css": "stylelint \"**/*.{css,scss}\"",
        "lint:css-fix": "stylelint \"**/*.{css,scss}\" --fix",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@hapi/iron": "^7.0.1",
        "axios": "^1.7.9",
        "cookies": "^0.9.1",
        "dotenv": "^16.4.7",
        "gsap": "^3.12.5",
        "lodash": "^4.17.21",
        "modern-normalize": "^3.0.1",
        "nanoid": "^5.0.9",
        "next": "15.1.6",
        "p5": "^1.11.2",
        "qrcode": "^1.5.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "stats.js": "^0.17.0",
        "three": "^0.171.0",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@swc/cli": "^0.5.2",
        "@swc/core": "^1.10.1",
        "eslint": "^9",
        "eslint-config-next": "15.1.6",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-next": "^0.0.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.0",
        "sass": "^1.84.0",
        "stylelint": "^16.14.1",
        "stylelint-config-prettier-scss": "^1.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "worklet-loader": "^2.0.0"
    }
}
